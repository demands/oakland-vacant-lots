{
  "name": "map-communities-example-frontend",
  "version": "0.0.0",
  "description": "An example map-communities frontend",
  "main": "app.js",
  "scripts": {
    "start": "static",
    "dev": "npm run watch & static",
    "build": "mkdir -p build && browserify app.js | uglifyjs - > build/app.js",
    "build-dev": "browserify app.js --debug | exorcist build/app.js.map > build/app.js",
    "watch": "watchy -w . -i 'build|.git|test' -- npm run build-dev",
    "prepublish": "npm run build",
    "test": "browserify test/index.js > test/_build.js && testling -x open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/map-communities/frontend.git"
  },
  "contributors": [
    "H0ke (http://h0ke.com/)",
    "demands (http://maxedmands.com)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/map-communities/frontend/issues"
  },
  "homepage": "https://github.com/map-communities/frontend",
  "dependencies": {
    "node-static": "^0.7.3",
    "browserify": "^3.44.2",
    "jquery": "^2.1.0",
    "mapbox.js": "^1.6.2",
    "leaflet.markercluster": "^0.4.0",
    "uglify-js": "^2.4.13",
    "mustache": "^0.8.1",
    "google-maps": "^2.0.0"
  },
  "devDependencies": {
    "exorcist": "^0.1.6",
    "watchy": "^0.4.3",
    "tape": "^2.13.1",
    "testling": "^1.6.1",
    "faucet": "0.0.1"
  },
  "testling": {
    "browsers": [
      "chrome/latest"
    ],
    "html": "test/test.html"
  }
}
